   "source": [
    "def chicken(x):\n",
    "    if x <= 1:\n",
    "        return 1\n",
    "    a, b = 1, 1\n",
    "    while True:\n",
    "        c = a + b\n",
    "        if c > x:\n",
    "            return a + chicken(x - b)\n",
    "        elif c == x:\n",
    "            return b\n",
    "        a, b = b, c"
   ]
